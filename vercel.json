{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/:path+.html", "destination": "/:path+", "permanent": true },
    { "source": "/:path+/$", "destination": "/:path+", "permanent": true }
  ],
  "rewrites": [
    { "source": "/events", "destination": "/events.html" },
    { "source": "/event/:slug", "destination": "/event.html" },
    { "source": "/news", "destination": "/news.html" },
    { "source": "/news/:slug", "destination": "/news-article.html" },
    { "source": "/:listingSlug/events", "destination": "/listing-events.html" }
  ]
}
